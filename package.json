{
  "name": "@wfp/common-identifier-algorithms",
  "version": "2.1.3",
  "description": "Generate psuedonymous identifiers from tabular data",
  "author": "World Food Programme",
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wfp/common-identifier-algorithms.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "exports": {
    "./generic": {
      "default": "./dist/_generic_hasher/index.js",
      "types": "./dist/_generic_hasher/index.d.js"
    },
    "./colombia": {
      "default": "./dist/colombia/index.js",
      "types": "./dist/colombia/index.d.js"
    },
    "./syria": {
      "default": "./dist/syria/index.js",
      "types": "./dist/syria/index.d.js"
    },
    "./syria_north_west": {
      "default": "./dist/syria_north_west/index.js",
      "types": "./dist/syria_north_west/index.d.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "COPYING"
  ],
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "postbuild": "tsx tools/process-configuration-files.ts --all",
    "clean": "git clean -xdf dist coverage",
    "clean:all": "git clean -xdf dist coverage node_modules",
    "pretest": "tsx tools/process-configuration-files.ts --all --no-bundle",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --config=jest.config.js"
  },
  "devDependencies": {
    "@wfp/common-identifier-algorithm-shared": "^2.1.5",
    "@commander-js/extra-typings": "^13.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "commander": "^13.1.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@wfp/common-identifier-algorithm-shared": "^2.1.5"
  }
}
